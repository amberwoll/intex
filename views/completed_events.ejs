<!-- Completed events and their stats -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Completed Events</title>
</head>
<body>
    <h1 style="text-align: center;">Completed Events</h1>
  <table>
    <thead>
      <tr>
        <th>Event Number</th>
        <th>Host Name</th>
        <th>Description</th>
        <th>Organization</th>
      </tr>
    </thead>
    <tbody>
      <% if (completed_events.length > 0) { %>
        <% completed_events.forEach(row => { %>
          <tr>
            <td><%= row.event_number %></td>
            <td><%= row.name %></td>
            <td><%= row.event_description %></td>
            <td><%= row.organization %></td>
            <td>
              <form action="/edit_completed_event/<%= row.event_number %>" method="GET" style="display:inline;">
                <button type="submit">Update</button>
              </form>
            </td>
          </tr>
        <% }) %>
      <% } else { %>
        <tr>
          <td colspan="7">No data available</td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>